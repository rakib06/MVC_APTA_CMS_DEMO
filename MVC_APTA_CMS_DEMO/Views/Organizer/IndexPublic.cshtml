@model IEnumerable<MVC_APTA_CMS_DEMO.Models.organizer>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>IndexPublic</title>
</head>
<body>
    @{Html.RenderAction("testNav", "Home");}
    <p>
        Organizer lists
    </p>
<table class="table table-bordered">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Location)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Website)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Phone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
           
        <th>
            @Html.DisplayNameFor(model => model.conferenceId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.reg_date)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Location)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Website)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Phone)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
               
               
            <td>
                @Html.DisplayFor(modelItem => item.conferenceId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.reg_date)
            </td>
            <td>
                    
                @Html.ActionLink("Details", "Details", new { id = item.OrganizerId }) 
                
            </td>
        </tr>
    }

</table>
    <p>
        <a href="@Url.Action("Create","TeamsPaper")" class="btn btn-default btn-lg btn-block" role="button">Create Team and join with us for upcoming Conference , 1 member as a delegates is allowed </a>

    </p>
</body>
</html>
